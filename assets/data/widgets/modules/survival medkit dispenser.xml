<?xml version="1.0"?>
<shipsystem sprite="91" name="survival medkit dispenser" health="10">
	<contains minimum="2" maximum="4">scrap metal</contains>
	<description>The survival medkit dispenser is a self contained system that can synthesize medical supplies with no external input.</description>
	<resistance resists="KINETIC" strength="2"></resistance>
	<shipResource capacity="1" resource="MEDKIT" nonCombat="true">
	</shipResource>
	<shipConverter>
		<convertFrom value="NOTHING" />
		<convertTo value="MEDKIT" />
		<conversionEfficiency value="1" />
		<conversionRate value="0.001" />
	</shipConverter>
	<!--dispenser goes here -->
	<shipDispenser>
		<input value="MEDKIT" />
		<cost value="1" />
		<output value="medkit" />
	</shipDispenser>
</shipsystem>